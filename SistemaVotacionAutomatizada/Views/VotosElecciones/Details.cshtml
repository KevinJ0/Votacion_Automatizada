@model SistemaVotacionAutomatizada.Models.VotosElecciones

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Details</h1>

<div>
    <h4>VotosElecciones</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Candidato)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Candidato.Apellido)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Ciudadano)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Ciudadano.Id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Eleccion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Eleccion.Nombre)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
